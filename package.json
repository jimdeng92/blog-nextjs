{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "dev": "cross-env NODE_ENV=development node server.js",
    "build": "next build",
    "export": "next build && next export -o outdir",
    "start": "cross-env NODE_ENV=production node server.js",
    "pm2": "cross-env NODE_ENV=production pm2 start ecosystem.config.js",
    "restart": "git pull && npm install && next build && pm2 restart blog-nextjs",
    "deploy": "./deploy.sh"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "axios": "^1.1.3",
    "color-themes-for-google-code-prettify": "^2.0.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^11.6.0",
    "http-proxy-middleware": "^1.0.6",
    "lucide-react": "^0.98.0",
    "marked": "^4.2.1",
    "next": "^13.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-toastify": "^9.1.1",
    "sass": "^1.32.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.0.2",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.22.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
